<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="chV.project.models.Moderators" table="moderators" schema="oneton">
        <id name="idMod">
            <column name="idMod" sql-type="int(11)"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>